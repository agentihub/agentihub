{
  "name": "agent-ihub",
  "version": "0.1.0",
  "description": "A GitHub-like platform for AI Agent configuration management",
  "type": "module",
  "license": "MIT",
  "author": "Agent iHub Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/agent-ihub.git"
  },
  "homepage": "https://github.com/yourusername/agent-ihub#readme",
  "bugs": {
    "url": "https://github.com/yourusername/agent-ihub/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "configuration",
    "management",
    "liteagent",
    "dify",
    "coze",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "generate-api": "openapi-ts",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "quality:check": "npm run type-check && npm run lint",
    "quality:fix": "npm run lint:fix && npm run format",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@chatscope/chat-ui-kit-react": "^2.1.1",
    "@hey-api/client-fetch": "0.2.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@primer/octicons-react": "^19.19.0",
    "@tanstack/react-query": "^5.90.5",
    "@types/node": "^24.0.8",
    "@uiw/react-md-editor": "^4.0.8",
    "antd": "^5.26.3",
    "dayjs": "^1.11.13",
    "eslint-config-prettier": "^10.1.5",
    "github-markdown-css": "^5.8.1",
    "immer": "^10.1.3",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.3",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@hey-api/openapi-ts": "0.52.3",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
